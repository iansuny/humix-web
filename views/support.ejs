<!DOCTYPE HTML>
<html>
	<head>
		<title>Support | Humix</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />

		<%- partial('components/scripts') %>
		
	</head>
	<body>

		<!-- Header -->
			<header id="header">
				<%- partial('components/navigation') %>
			</header>

		<!-- Main -->
			<section id="main" class="wrapper style4">
				<div class="container box big">

					<header class="major special">
						<h2>Support</h2>
						<p>Leave your messenge and we will get you as soon as possible.</p>
					</header>

					<form method="post" action="#">
						<div class="row uniform 50%">
							<div class="6u 12u$(xsmall)">
								<input type="text" name="name" id="name" value="" placeholder="Name">
							</div>
							<div class="6u$ 12u$(xsmall)">
								<input type="email" name="email" id="email" value="" placeholder="Email">
							</div>
							<div class="12u$">
								<div class="select-wrapper">
									<select name="category" id="category">
										<option value="">- Category -</option>
										<option value="suggestion">Provide a Suggestion</option>
										<option value="bug">Report a Bug</option>
										<option value="cooperation">Ask for Cooperation</option>
										<option value="others">Others</option>
									</select>
								</div>
							</div>
							<div class="12u$">
								<textarea name="message" id="message" placeholder="Enter your message" rows="6"></textarea>
							</div>
							<div class="12u$">
								<ul class="actions">
									<li><input type="submit" value="Send Message" class="special"></li>
									<li><input type="reset" value="Reset"></li>
								</ul>
							</div>
						</div>
					</form>

				</div>
			</section>

		<%- partial('components/footer') %>

	</body>
</html>